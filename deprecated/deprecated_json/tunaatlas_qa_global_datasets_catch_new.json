{
  "profile": {
    "id": "tuna-atlas-catch",
    "mode": "entity",
    "project": "FIRMS Tuna Atlas",
    "organization": "FIRMS Tuna Atlas",
    "logos": [
      "http://firms.fao.org/figis/website/assets/images/templates/firms/firms_banner.gif",
      "https://www.blue-cloud.org/sites/default/files/styles/logo_small/public/FAO_2lines_500x500_1.png?itok=igwPSL5v",
      "https://en.ird.fr/sites/ird_fr/files/2019-08/logo_IRD_2016_BLOC_UK_COUL.png"
    ],
    "options": {
      "line_separator": "_\n"
    }
  },
  "metadata": {
    "entities" : [
      {
      "handler": "gsheet",
      "source": "https://docs.google.com/spreadsheets/d/1JhS1ADsnqTpWOEEvljk-c9EdzBurkc8A8__JOHJmkOk/edit#gid=1243274109"
      }
    ],
    "contacts" : [
      {
        "handler": "gsheet",
        "source": "https://docs.google.com/spreadsheets/d/1DtGR8cHAkro12DOFZn40cehfU9LoI9l7wVUCHfHH-xY/edit?usp=sharing"
      }
    ]
  },
  "software": [
    {
      "id": "my-database-read",
      "type": "input",
      "software_type": "dbi",
      "parameters": {
        "drv": "PostgreSQL",
        "user": "{{DB_USER}}",
        "password": "{{DB_PASSWORD}}",
        "host": "{{DB_HOST}}",
        "port": "{{DB_PORT}}",
        "dbname": "{{DB_NAME}}"
      },
      "properties" : {}
    },
    {
      "id": "my-database-write",
      "type": "output",
      "software_type": "dbi",
      "parameters": {
        "drv": "PostgreSQL",
        "user": "{{DB_USER}}",
        "password": "{{DB_PASSWORD}}",
        "host": "{{DB_HOST}}",
        "port": "{{DB_PORT}}",
        "dbname": "{{DB_NAME}}"
      },
      "properties" : {}
    },
    {
      "id": "googledrive",
      "type": "input",
      "software_type": "googledrive",
      "parameters": {
        "email": "{{GMAIL_USER}}",
        "token": "{{GMAIL_TOKEN}}"
      },
      "properties" : {}
    }
  ],
  "actions": [
  
	
	{
		"id": "load_dataset",
		"script": "./R/tunaatlas_actions/load_dataset.R",
		"run": false,
		"options": {
			"upload_to_db": false,
			"create_materialized_view" : false,
			"add_sql_comments" : false,
			"upload_to_googledrive": false
		}
	},
	{
		"id": "enrich_metadata",
		"script": "https://raw.githubusercontent.com/eblondel/geoflow-tunaatlas/master/R/tunaatlas_actions/enrich_metadata.R",
		"run": false
	},
	{
		"id": "enrich_for_db_services",
		"script": "https://raw.githubusercontent.com/eblondel/geoflow-tunaatlas/master/R/tunaatlas_actions/enrich_db_for_services.R",
		"run": false
	},
	{
		"id": "load_metadata",
		"script": "https://raw.githubusercontent.com/eblondel/geoflow-tunaatlas/master/R/tunaatlas_actions/load_metadata.R",
		"run": false
	},
	{		"id": "recap_all_markdown",
		"script": "https://raw.githubusercontent.com/BastienIRD/Tunaatlas_level1/main/recap_all_markdown.R",
		"run": false
	}
	

 ]
}
